@model WebApplication2.Models.MixmodelCal
<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1>Llenado de Indicadores</h1>
                    <h1 style="color:darkslateblue">@ViewData["message"]</h1>
                    <h1 style="color:red">@ViewData["message1"]</h1>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="#">Home</a></li>
                        <li class="breadcrumb-item active">Llenado de indicadores</li>
                    </ol>
                </div>
            </div>
        </div><!-- /.container-fluid -->
    </section>

    <!-- Main content -->
    <form asp-action="ULlenadoIndicadoresCal" method="post" enctype="multipart/form-data">
        <section class="content">
            <div class="container-fluid">
                <div class="row">
                    <!-- left column -->
                    <div class="col-md-12">
                        <!-- jquery validation -->
                        @*<label>Planta</label>
                        <select asp-for="BDQuejasA.Region">
                            <option value="N">Seleccione una opción</option>
                            <option value="Jalisco">DV</option>
                            <option value="Toluca">RT</option>
                            <option value="San Luis Potosí"> RSLP</option>
                            <option value="Matehuala">RMTH</option>
                        </select>*@
                        <div class="card card-primary">
                            <div class="card-header">
                                <h3 class="card-title">Llenado de indicadores <small>  Calidad</small></h3>
                            </div>
                            <!-- /.card-header -->
                            <!-- form start -->
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label for="exampleInputFile">CALIFICACIÓN DE PRODUCTO</label>
                                            <div class="input-group">
                                                <div class="custom-file">
                                                    <input type="file" name="fileCalifProd" class="custom-file-input" id="exampleInputFile"required="required">
                                                    <label class="custom-file-label" for="exampleInputFile">Choose file</label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label for="exampleInputRounded0">QUEJAS A<code></code></label>
                                            <input asp-for="BDQuejasA.QuejasA" class="form-control"required="required" />
                                            <span asp-validation-for="BDQuejasA.QuejasA" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label for="exampleInputRounded0">QUEJAS TOTALES<code></code></label>
                                            <input asp-for="BDQuejasTotale.QuejasTotales" class="form-control"required="required" />
                                            <span asp-validation-for="BDQuejasTotale.QuejasTotales" class="text-danger"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label for="exampleInputFile">CP Y CPK de peso</label>
                                            <div class="input-group">
                                                <div class="custom-file">
                                                    <input type="file" name="fileCPCPKPeso" class="custom-file-input" id="exampleInputFile">
                                                    <label class="custom-file-label" for="exampleInputFile">Choose file</label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label for="exampleInputRounded0">QUEJAS B<code></code></label>
                                            <input asp-for="BDQuejasB.QuejasB" class="form-control"required="required" />
                                            <span asp-validation-for="BDQuejasB.QuejasB" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label for="exampleInputFile">RECHAZOS DE PT</label>
                                            <div class="input-group">
                                                <div class="custom-file">
                                                    <input type="file" name="fileRechazosPT" class="custom-file-input" id="exampleInputFile">
                                                    <label class="custom-file-label" for="exampleInputFile">Choose file</label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label for="exampleInputFile">CP y CPK de proceso</label>
                                            <div class="input-group">
                                                <div class="custom-file">
                                                    <input type="file" name="fileCPCPKProceso" class="custom-file-input" id="exampleInputFile">
                                                    <label class="custom-file-label" for="exampleInputFile">Choose file</label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label for="exampleInputRounded0">QUEJAS C<code></code></label>
                                            <input asp-for="BDQuejasC.QuejasC" class="form-control"required="required" />
                                            <span asp-validation-for="BDQuejasC.QuejasC" class="text-danger"></span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="card-footer">
                                <div>
                                    <input type="submit" value="Guardar" class="btn btn-primary" />
                                </div>
                            </div>
                        </div></div>
                    <!-- /.card -->
                </div>
                <!--/.col (left) -->
                <!-- right column -->
                <!--/.col (right) -->
            </div>
        </section>
        <!-- /.content -->
    </form>
</div>