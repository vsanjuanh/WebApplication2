@model WebApplication2.Models.BdAccidente
<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1>Formularios</h1>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="#">Home</a></li>
                        <li class="breadcrumb-item active">Llenado de indicadores</li>
                    </ol>
                </div>
            </div>
        </div><!-- /.container-fluid -->
    </section>
    <!-- Main content -->
    <section class="content">
        <div class="container-fluid">
            <div class="row">
                <!-- left column -->
                <div class="col-md-12">
                    <!-- Small Box (Stat card) -->
                    <div class="row">
                        <div class="col-lg-3 col-6">
                            <!-- small card -->
                            <div class="small-box bg-info">
                                <div class="inner">
                                    <h4>Producción%</h4>
                                    <p>-------</p>
                                </div>
                                <div class="icon">
                                    <i class="fas ion-stats-bars"></i>
                                </div>
                                <a href="@Url.ActionLink("ULlenadoIndicadoresProd","BdAccidentes")" class="small-box-footer">
                                    Ir a formularios <i class="fas fa-arrow-circle-right"></i>
                                </a>
                            </div>
                        </div>
                        <!-- ./col -->
                        <div class="col-lg-3 col-6">
                            <!-- small card -->
                            <div class="small-box bg-success">
                                <div class="inner">
                                    <h4>Calidad%</sup></h4>

                                    <p>-------</p>
                                </div>
                                <div class="icon">
                                    <i class="fas fa-flask"></i>
                                </div>
                                <a href="@Url.ActionLink("ULlenadoIndicadoresCal","BdAccidentes")" class="small-box-footer">
                                    Ir a formularios <i class="fas fa-arrow-circle-right"></i>
                                </a>
                            </div>
                        </div>
                        <!-- ./col -->
                        <div class="col-lg-3 col-6">
                            <!-- small card -->
                            <div class="small-box bg-danger">
                                <div class="inner">
                                    <h4>Mantenimiento</h4>
                                    <p>----------</p>
                                </div>
                                <div class="icon">
                                    <i class="fas fa-tools"></i>
                                </div>
                                <a href="@Url.ActionLink("ULlenadoIndicadoresMant","BdAccidentes")" class="small-box-footer">
                                    Ir a formularios <i class="fas fa-arrow-circle-right"></i>
                                </a>
                            </div>
                        </div>
                        <!-- ./col -->
                        <div class="col-lg-3 col-6">
                            <!-- small card -->
                            <div class="small-box bg-gradient-cyan">
                                <div class="inner">
                                    <h4>SEGA</h4>

                                    <p>----------</p>
                                </div>
                                <div class="icon">
                                    <i class="fas fa-hands-wash"></i>
                                </div>
                                <a href="@Url.ActionLink("ULlenadoIndicadoresSeg","BdAccidentes")" class="small-box-footer">
                                    Ir a formularios <i class="fas fa-arrow-circle-right"></i>
                                </a>
                            </div>
                        </div>
                        <!-- ./col -->
                    </div>
                    <!-- /.row -->
                    <!-- /.card -->
                </div>
                <!--/.col (left) -->
                <!-- right column -->
                <!--/.col (right) -->
            </div>
            <!-- /.row -->
        </div><!-- /.container-fluid -->
    </section>
    <!-- /.content -->
</div>
<!-- /.content-wrapper -->
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
